<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="pagePane" prefHeight="40.0" prefWidth="260.0" style="-fx-background-color: #CCFFFF;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextField fx:id="option" maxWidth="100.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <Button fx:id="previous" alignment="CENTER" mnemonicParsing="false" text="上一页" AnchorPane.leftAnchor="120.0" AnchorPane.topAnchor="10.0"/>
        <TextField fx:id="page" maxWidth="30.0" AnchorPane.leftAnchor="180.0" AnchorPane.topAnchor="10.0"/>
        <Button fx:id="next" mnemonicParsing="false" text="下一页" AnchorPane.leftAnchor="220.0" AnchorPane.topAnchor="10.0"/>
        <Text fx:id="totalPage" strokeType="OUTSIDE" strokeWidth="0.0" AnchorPane.leftAnchor="280.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="13.0"/>
    </children>
</AnchorPane>
