<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="pagePane" prefHeight="40.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="previous" mnemonicParsing="false" text="上一页" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
        <TextField fx:id="page" maxWidth="30.0" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="10.0" />
        <Button fx:id="next" mnemonicParsing="false" text="下一页" AnchorPane.leftAnchor="110.0" AnchorPane.topAnchor="10.0" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="每页" AnchorPane.leftAnchor="180.0" AnchorPane.topAnchor="13.0" />
        <ChoiceBox maxWidth="50.0" AnchorPane.leftAnchor="205.0" AnchorPane.topAnchor="10.0" />
    </children>
</AnchorPane>
